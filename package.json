{
  "name": "jstextfromimage",
  "version": "0.2.0",
  "description": "Get descriptions of images from OpenAI, Azure OpenAI, and Anthropic Claude models in an easy way.",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "build": "echo \"No build step required.\"",
    "prepare": "npm run build"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OrenGrinker/jstextfromimage.git"
  },
  "keywords": [
    "openai",
    "gpt-4",
    "claude",
    "azure-openai",
    "computer-vision",
    "image-to-text",
    "ai",
    "machine-learning"
  ],
  "author": "Oren Grinker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OrenGrinker/jstextfromimage/issues"
  },
  "homepage": "https://github.com/OrenGrinker/jstextfromimage#readme",
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.11",
    "@anthropic-ai/sdk": "^0.18.0",
    "axios": "^1.3.0",
    "dotenv": "^16.0.0",
    "mime-types": "^2.1.34",
    "openai": "^4.73.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^8.0.0",
    "jest": "^29.7.0"
  }
}